package Sokoban;

import java.awt.Graphics;

public class Sokoban extends GUI {
	
	public Sokoban(){}
	
	public void onMove(Move m){
		if(m==Move.LEFT){
			getTextOutputArea().append("You Move Left\n");
			
		}
		
	}
	
	public void redraw(Graphics g){
		
	}
	
	public static void main(String[] args){
		new Sokoban();
	}

}
